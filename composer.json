{
    "name": "asaliev/symfony-yii2-bridge",
    "version": "0.1.0",
    "type": "symfony-bundle",
    "homepage": "https://github.com/asaliev/symfony-yii2-bridge",
    "keywords": [
        "symfony",
        "yii2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Timur Asaliev"
        }
    ],
    "require": {
        "php": ">=7.4 || ^8.0",
        "symfony/framework-bundle": "^5.4 | ^6 || ^7",
        "symfony/messenger": "^5.4 | ^6 || ^7",
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "yidas/yii2-composer-bower-skip": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^7.2",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "squizlabs/php_codesniffer": "^3.12",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Asaliev\\Yii2Bridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asaliev\\Tests\\Yii2Bridge\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs src/",
        "phpstan-74": "vendor/bin/phpstan analyse -c phpstan.74.neon --no-progress --memory-limit 512M",
        "phpstan-80": "vendor/bin/phpstan analyse -c phpstan.80.neon --no-progress --memory-limit 512M",
        "test": "vendor/bin/phpunit --testdox --no-interaction"
    }
}
